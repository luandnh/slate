    "campaign_uuid": "avavavav-1111-2222-3333-eeeeeeee",
    "carrier": "mobi",
    "template": "thong_bao_no_cuoc_01",
    "concurrent_call": 2,
    "customers" : [
      {
        "phone_number": "0899123456",
        "params": {
            "customer_name" : "Nguyễn Văn A",
            "contract_number": "HD123456",
            "due_date": "2021-07-13"
        }
      }
    ]
}'</span>
</code></pre></div>
